class Person:
    def __init__(self, name=None, age=None):
        if name is None:
            self.name = input("Enter name: ")
        else:
            self.name = name

        if age is None:
            self.age = int(input("Enter age: "))
        else:
            self.age = age

    def display(self):
        print("Name:", self.name)
        print("Age:", self.age)

p1 = Person()
p1.display()

p2 = Person("Alice", 25)
p2.display()

[ THE OUTPUT OF THE PROGRAM.
  Enter name: vasanth
Enter age: 22
Name: vasanth
Age: 22
Name: Alice
Age: 25 ]
