def is_prime(n):
    if n <= 1:
        return False
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            return False
    return True

print("Prime numbers between 1 and 10 are:")
for num in range(1, 11):
    if is_prime(num):
        print(num)

print("Prime numbers between 20 and 30 are:")
for num in range(20, 31):
    if is_prime(num):
        print(num)
        
        [  OUTPUT OF THE PROGRAM. 
           
           Prime numbers between 1 and 10 are:
2
3
5
7
Prime numbers between 20 and 30 are:
23
29
    ]
